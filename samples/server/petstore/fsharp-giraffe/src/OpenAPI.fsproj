<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Description>OpenAPI</Description>
    <Copyright>OpenAPI</Copyright>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <DebugType>portable</DebugType>
    <EnableDefaultContentItems>false</EnableDefaultContentItems>
    <RunWorkingDirectory>$(MSBuildThisFileDirectory)</RunWorkingDirectory>
    <AssemblyName>OpenAPI</AssemblyName>
    <PackageId>OpenAPI</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.All" />
    <PackageReference Include="Giraffe" Version="3.4.*" />
    <PackageReference Include="OpenAPITypeProvider" Version="2.3.0" />
    <PackageReference Include="TaskBuilder.fs" Version="2.1.*" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="openapi.yaml"/>
    <Compile Include="Model.fs" />
    <Compile Include="api/params/UrlParams.fs" />
    <Compile Include="api/params/QueryParams.fs" />
    <Compile Include="api/params/BodyParams.fs" />
    <Compile Include="api/params/HeaderParams.fs" />
    <Compile Include="api/PetApi.fs" />
    <Compile Include="api/StoreApi.fs" />
    <Compile Include="api/UserApi.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

</Project>